var jcd;var gaobaomoban;$(function() {	reportPreviewInitData();});function reportPreviewInitData(){	jcd = parameter.jcd;	baogaomoben = parameter.baogaomoben;	// 申请医生下拉框赋值	if ($("#rep_doc").val() == null) {		$("#rep_doc").val() =="-";	}else{		var data_getKaiDanDoctorByQuanxian = getJSONData(				"/publish/yuangong/getKaiDanDoctorByQuanxian.htm", {					tag : Math.random()				}, "post");		if (data_getKaiDanDoctorByQuanxian.state) {			var yuangonglist = data_getKaiDanDoctorByQuanxian.obj;			$.each(yuangonglist, function(i, yuangong) {				var option=$(						"<option value=\"" + yuangong.gonghao + "\">"								+ yuangong.xingming + "</option>").appendTo(						"#rep_doc");				if(yuangong.gonghao==jcd.kdys){					option.attr("selected","selected");				}			});		}	}	// 申请医生下拉框赋值	// 检查医生下拉框赋值	if ($("#doctor").length == 1) {		var data_getJianChaDoctorByBumenAndQuanxian = getJSONData(				"/publish/yuangong/getJianChaDoctorByBumenAndQuanxian.htm", {// 获取具有检查权限的医生					tag : Math.random()				}, "post");		if (data_getJianChaDoctorByBumenAndQuanxian.state) {			var yuangonglist = data_getJianChaDoctorByBumenAndQuanxian.obj;			$.each(yuangonglist, function(i, yuangong) {				$(						"<option value=\"" + yuangong.gonghao + "\">"								+ yuangong.xingming + "</option>").appendTo(						"#doctor");			});		}	}	// 医生下拉框赋值	// 初始化患者信息和检查单的基本信息	var url_outBaogaoHelp = "/publish/baogao/outBaogaoHelp.htm";// 查询该报告页面所有元素信息	var patameter_outBaogaoHelp = {		id : jcd.id,// 检查单ID		huanzheId : jcd.huanzheId,// 患者ID		tag : Math.random()	};	var data_outBaogaoHelp = getJSONData(url_outBaogaoHelp,			patameter_outBaogaoHelp, "post");	if (data_outBaogaoHelp.state) {		initial_Baogao(data_outBaogaoHelp.obj);// 报告页面各元素赋值	}	// 初始化患者信息和检查单的基本信息//	if () {  //眼底照相，眼底照相（立体照相）//		initData_eyeydzx();//	} else		 if (parameter.jcd.jcxmIds == json_jcxm.ydzx_litizhaoxiang				 || parameter.jcd.jcxmIds == json_jcxm.ydzx_jfw || parameter.jcd.jcxmIds == json_jcxm.ydzx||parameter.jcd.jcxmIds == json_jcxm.ygzy_yinduoqinglv			|| parameter.jcd.jcxmIds == json_jcxm.ygzy_yiguangsunan //眼底荧光造影（立摄得）,眼底荧光造影（荧光素钠）,眼底荧光造影（吲哚青绿）			|| parameter.jcd.jcxmIds == json_jcxm.ygzy_lishede			|| parameter.jcd.jcxmIds == json_jcxm.zfygFFA			|| parameter.jcd.jcxmIds == json_jcxm.zfyg_jfw			|| parameter.jcd.jcxmIds == json_jcxm.zfygICGA) {		initData_eyeygzy();	}else if (parameter.jcd.jcxmIds == json_jcxm.shiye			|| parameter.jcd.jcxmIds == json_jcxm.weishiye) {//视野检查（进口视野计）,偏振激光(GDX)眼底扫描(微视野)		initData_eyegcsy();	} else if (parameter.jcd.jcxmIds == json_jcxm.oct_huangbangqu_shipan			|| parameter.jcd.jcxmIds == json_jcxm.oct_huangbangqu //前节OCT),(OCT)(部位：视盘),(OCT)(部位：黄斑区),OCT)(部位：黄斑区+视盘)			|| parameter.jcd.jcxmIds == json_jcxm.oct_shipan			|| parameter.jcd.jcxmIds == json_jcxm.oct_qianjie			|| parameter.jcd.jcxmIds == json_jcxm.oct_xlcx) {		initData_eyeoct();	}else if(parameter.jcd.jcxmIds==json_jcxm.qianfangshendu){		initData_eyeqianfangshendu();	} 	else  if (parameter.jcd.jcxmIds == json_jcxm.anshiyingjiancha||parameter.jcd.jcxmIds == json_jcxm.gfs) {//暗适应测试		initData_eyejmdxt_an();	} else if (parameter.jcd.jcxmIds == json_jcxm.jiaomoneipijishu) {//角膜内皮镜检查+录像记录		initData_eyejmspjs();	} else if (parameter.jcd.jcxmIds == json_jcxm.jiaomoqulvji) {//角膜曲率		initData_eyejmqlj();	} else if (parameter.jcd.jcxmIds == json_jcxm.qianfangjiaojing) {//前房角镜检查（收三面镜检查费）		initData_eyeqfjj();	} else if (parameter.jcd.jcxmIds == json_jcxm.jiufangwei) {//九方位斜视检查		initData_eyetsjjfw();	} else if (parameter.jcd.jcxmIds == json_jcxm.sanjishigongneng) {//同视机双眼视觉		initData_eyetsjsj();	} else if (parameter.jcd.jcxmIds == json_jcxm.yanya) {//非接触眼压检查		initData_eyeballpress();	} else if (parameter.jcd.jcxmIds == json_jcxm.bchao) {//眼科B超		initData_eyebchao();	} else if (parameter.jcd.jcxmIds == json_jcxm.yinxiejiancha//隐斜检查,马氏(Maddox)杆试验			|| parameter.jcd.jcxmIds == json_jcxm.mashigan) {		initData_eyeyxjc();	} else if (parameter.jcd.jcxmIds == json_jcxm.sanmianjing) {//三面镜眼底检查		initData_eyesmj();	} else if (parameter.jcd.jcxmIds == json_jcxm.ubm) {//超声生物显微镜(UBM)		initData_eyeubm();	} else if (parameter.jcd.jcxmIds == json_jcxm.achao			|| parameter.jcd.jcxmIds == json_jcxm.iolmaster			|| parameter.jcd.jcxmIds == json_jcxm.rgjtdsjs			|| parameter.jcd.jcxmIds == json_jcxm.yzcl			|| parameter.jcd.jcxmIds == json_jcxm.yzrgjtdscl_csf) {// A超、眼轴人工晶状体度数测量-光学法、//眼轴人工晶状体度数测量-超声法、眼轴测量		initData_eyect();	} else if (parameter.jcd.jcxmIds == json_jcxm.dsl_fvep) { //电生理-fvep		initData_eyeFVEP();	} else if (parameter.jcd.jcxmIds == json_jcxm.dsl_pvep) { //电生理-pvep		initData_eyePVEP();	} else if (parameter.jcd.jcxmIds == json_jcxm.dsl_ferg) { //电生理-ferg		initData_eyeFERG();	} else if (parameter.jcd.jcxmIds == json_jcxm.dsl_perg) { ////电生理-perg		initData_eyePERG();	} else if (parameter.jcd.jcxmIds == json_jcxm.dsl_mf_erg) { //电生理-mf-erg		initData_eyeMfERG();	} else if (parameter.jcd.jcxmIds == json_jcxm.dsl_eog) { //电生理-eog		initData_eyeEOG();	}else if (parameter.jcd.jcxmIds == json_jcxm.ktYanguang			|| parameter.jcd.jcxmIds == json_jcxm.xtYanguang||parameter.jcd.jcxmIds==json_jcxm.xtygedtrs) {// 小瞳验光、扩瞳验光		initData_eyeygnew();	}else if (parameter.jcd.jcxmIds == json_jcxm.tiaojiefucou) { //调节辐辏		initData_eyexsjc();	}else if (parameter.jcd.jcxmIds == json_jcxm.sanlengjing			|| parameter.jcd.jcxmIds == json_jcxm.xieshiduceding) {// 三棱镜,斜视度		initData_eyeslj();	} else if (parameter.jcd.jcxmIds == json_jcxm.teshushili			||parameter.jcd.jcxmIds == json_jcxm.tuxingshili) { //特殊视力、图形视力检查		initData_eyetsli();	}else if (parameter.jcd.jcxmIds == json_jcxm.sejuejiancha) {// 色觉检查		initData_eyesjjc();	}else if (parameter.jcd.jcxmIds == json_jcxm.liexidengzhaoxiang) {// 裂隙灯照相		initData_eyelxdzx();	}else if (parameter.jcd.jcxmIds == json_jcxm.dishilizhushiqi) { //低视力助视器		initData_eyedslzsq();	}else if (parameter.jcd.jcxmIds == json_jcxm.jiguangzhiliao//激光治疗			 ||parameter.jcd.jcxmIds == json_jcxm.jgqnmsjs//激光前囊膜松解术			 ||parameter.jcd.jcxmIds == json_jcxm.hmzbqcs//虹膜周边切除术			 ||parameter.jcd.jcxmIds == json_jcxm.swmdbcjggn//视网膜多波长激光光凝			 ||parameter.jcd.jcxmIds == json_jcxm.pdt//光动力疗法（PDT）			 ||parameter.jcd.jcxmIds == json_jcxm.ttt//经瞳孔温热疗法（TTT）			 ||parameter.jcd.jcxmIds == json_jcxm.jghnmqks//激光后囊膜切开术			 ||parameter.jcd.jcxmIds == json_jcxm.khjggmbfxqds//氪黄激光巩膜瓣缝线切断术			 ||parameter.jcd.jcxmIds == json_jcxm.adkscmqks//安顿孔渗出膜切开术			 ||parameter.jcd.jcxmIds == json_jcxm.tkqscmqks//瞳孔区渗出膜切开术			 ||parameter.jcd.jcxmIds == json_jcxm.jgzlqgy//激光治疗青光眼（SLT）			 ||parameter.jcd.jcxmIds == json_jcxm.jghmbmxsxgfbs//激光虹膜表面新生血管封闭术			 ||parameter.jcd.jcxmIds == json_jcxm.yagjghmcmqks			 ||parameter.jcd.jcxmIds == json_jcxm.jghmzbcxs) { // 激光治疗		initData_eyejgzl();	}else if (parameter.jcd.jcxmIds == json_jcxm.yanweiyanji) {// 眼位眼肌		initData_eyeywyj();	}else if (parameter.jcd.jcxmIds == json_jcxm.ruoshizhiliao) { //弱视治疗		initData_eyetsjzl();	}else if (parameter.jcd.jcxmIds == json_jcxm.jiaomohoudu) {// 角膜厚度		initData_eyejmhd();	}else if (parameter.jcd.jcxmIds == json_jcxm.zhushixingzhi			  ||parameter.jcd.jcxmIds == json_jcxm.yizhiandian ) {// 注释性质，抑制暗点		initData_eyezsxz();	}else if (parameter.jcd.jcxmIds == json_jcxm.leidaochongxi) {// 泪道冲洗		initData_eyeldcx();	}else if (parameter.jcd.jcxmIds == json_jcxm.fushijiancha) {// 复视检查		initData_eyejm();	}else if (parameter.jcd.jcxmIds == json_jcxm.budengxiang) {// 不等像检查		initData_eyebdx();	}else if(parameter.jcd.jcxmIds == json_jcxm.duibimingandu||parameter.jcd.jcxmIds==json_jcxm.jiaomodixingtu||parameter.jcd.jcxmIds==json_jcxm.jmdxtsirius){//对比敏感度		initData_eyemingandu();	}else if(parameter.jcd.jcxmIds == json_jcxm.retcam){		initData_eyeRetcam();	}}function initData_eyemingandu() {	/** ********************************敏感度信息********************************** */	var url_selectEyeminganduByEyemingandu = "/publish/Eyemingandu/selectEyeminganduByEyemingandu.htm";// 根据敏感度报告对象查询符合条件的敏感度报告对象	var data_selectEyeminganduByEyemingandu = getJSONData(			url_selectEyeminganduByEyemingandu, {				jcdId : jcd.id,				tag : Math.random()			}, "post");	if (data_selectEyeminganduByEyemingandu.state) {		//showReportPictures();// 显示报告图片		var eyemingandu = data_selectEyeminganduByEyemingandu.obj;		var rep_doc_name = $("#rep_doc option:selected").text();		var doctor_name = $("#doctor option:selected").text();		if (eyemingandu != null) {			if ($("#memo").length == 1) {				var demo = eyemingandu.memo;				while(demo.indexOf(" ")!=-1)					demo = demo.replace(" ","&nbsp;&nbsp;");				demo = demo.replace(/\r|\n/g,"<br />");				$("#memo").replaceWith(						$("<span/>").attr("id","memo").html(demo));			}			if ($("#cli_date").length == 1)				$("#cli_date").html(eyemingandu.cli_date);// 报告日期			//申请医生			if ($("#rep_doc").length == 1) 				$("#rep_doc").replaceWith(						$("<span/>").attr("id", "rep_doc").html(								rep_doc_name));			//检查医生			if ($("#doctor").length == 1) 				$("#doctor").replaceWith(						$("<span/>").attr("id", "doctor").html(								doctor_name));		}	}	/** ********************************敏感度信息********************************** */}//不等像检查报告function initData_eyebdx(){	var url_selectEyebdxByEyebdx = "/publish/Eyebdx/selectEyebdxByEyebdx.htm";	var repDoc_name = $("#rep_doc option:selected").text();// 申请医生	var doctor_name = $("#doctor option:selected").text();// 检查医生	var data_selectEyebdxByEyebdx = getJSONData(			url_selectEyebdxByEyebdx, {				jcdId :jcd.id,				tag : Math.random()			}, "post");	if (data_selectEyebdxByEyebdx.state) {		var eyebdx = data_selectEyebdxByEyebdx.obj;		if (eyebdx != null) {			if ($("#ls_r").length == 1)//激光类型				$("#ls_r").replaceWith($("<span/>").attr("id","ls_r").html(						eyebdx.ls_r));			if ($("#ls_l").length == 1)				$("#ls_l").replaceWith($("<span/>").attr("id","ls_l").html(						eyebdx.ls_l));			if ($("#dj_r").length == 1)				$("#dj_r").replaceWith($("<span/>").attr("id","dj_r").html(						eyebdx.dj_r));			if ($("#dj_l").length == 1)				$("#dj_l").replaceWith($("<span/>").attr("id","dj_l").html(						eyebdx.dj_l));			//申请医生			if ($("#rep_doc").length == 1) 				$("#rep_doc").replaceWith(						$("<span/>").attr("id", "rep_doc").html(								repDoc_name));			//检查医生			if ($("#doctor").length == 1) 				$("#doctor").replaceWith(						$("<span/>").attr("id", "doctor").html(								doctor_name));		}	}}//复视检查function showFushiFlash(){	importJS("/js/jquery.swfobject.1-1-1.min.js");	var params = {			allowFullScreen : true,			allowScriptAccess : "sameDomain",			quality : "high",			wmode : "transparent"		};	fushiFlash= $("#fushiReport").flash({		swf : contextPath+"/swf/fushiReport.swf",		id : "fushiReportSWF" ,		width : $("#fushiReport").width(),		height : $("#fushiReport").height(),		flashvars : {savePhotoCallback:"fushiSaveCallback"},		paremeters : params	});}function initData_eyejm(){	var url_selectEyejmByEyejm = "/publish/Eyejm/selectEyejmByEyejm.htm";	var repDoc_name = $("#rep_doc option:selected").text();// 申请医生	var doctor_name = $("#doctor option:selected").text();// 检查医生	var data_selectEyejmByEyejm = getJSONData(			url_selectEyejmByEyejm, {				jcdId : jcd.id,				tag : Math.random()			}, "post");	if (data_selectEyejmByEyejm.state) {		var eyejm = data_selectEyejmByEyejm.obj;		if (eyejm != null) {			if ($("#result").length == 1)				$("#result").replaceWith($("<span/>").attr("id","result").attr("text-align","left").html(						eyejm.result));			//申请医生			if ($("#rep_doc").length == 1) 				$("#rep_doc").replaceWith(						$("<span/>").attr("id", "rep_doc").html(								repDoc_name));			//检查医生			if ($("#doctor").length == 1) 				$("#doctor").replaceWith(						$("<span/>").attr("id", "doctor").html(								doctor_name));			if (eyejm.fsurl && eyejm.fsurl.length){				var img = $("<img />").dblclick(function(){					$("#fushiReport").text("");					showFushiFlash();				}).attr("src",contextPath+eyejm.fsu