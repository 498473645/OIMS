.tongzhiShowDivBg{
	position:absolute;
	background:#fff;
	border:1px solid #ccc;
	filter:alpha(opacity=50);
	-moz-opacity:0.5;
	opacity:0.5;
}

.tongzhiShowDiv h1{text-indent:1em; background:url("../images/titlebg.png") repeat-x; height:22px; line-height:22px; font-size:14px}
.tongzhiShowDiv ul{margin:0; padding:8px}
.tongzhiShowDiv ul li{list-style:none; float:none;}
.tongzhiShowDiv ul li a:hover{color:#ff0000}
.tongzhiShowDiv ul li span.new, .tongzhiShowDiv td span.new{color:#ff0000; font-size:10px;}
.tongzhiShowDiv table{width:100%}
.tongzhiShowDiv table th{background:#ddd}
.tongzhiShowDiv table td{border:1px sold #ddd; text-align:center}
.tongzhiShowDiv table tr.curr td{background:#bbe5ea}

.showTableData{border:1px solid #ddd; background:#fff}
.showTableData td{text-align:left; vertical-align:top}

td.formTdTitle{text-align:right; font-weight:bold; width:88px}
td input.inputCommon{width:100%;}
td textarea.inputCommon{width:100%}